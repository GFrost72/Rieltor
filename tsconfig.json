{
    "extends": "astro/tsconfigs/base",
    "compilerOptions": {
      "baseUrl": "./src",
      "paths": {
        "@styles/*": [
          "styles/*"
        ],
        "@pages/*": [
          "pages/*"
        ],
        "@components/*": [
          "components/*"
        ],
        "@sections/*": [
          "sections/*"
        ],
        "@layouts/*": [
          "./layouts/*"
        ],
        "@utils/*": [
            "./utils/*"
          ]
      },
      "jsx": "react-jsx",
      "jsxImportSource": "react"
    },
    "include": [
      "src"
    ],
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
